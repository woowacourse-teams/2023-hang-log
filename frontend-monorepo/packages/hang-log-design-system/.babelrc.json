{
  "sourceType": "unambiguous",
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "chrome": 100
        }
      }
    ],
    "@babel/preset-typescript",
    "@babel/preset-react",
    "@emotion/babel-preset-css-prop"
  ],
  "plugins": ["@emotion", 
      [
        "@babel/plugin-transform-react-jsx",
        { "runtime": "automatic", "importSource": ["@emotion/react"] }
      ],
    "react-require"
  ]
}
